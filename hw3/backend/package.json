{
  "name": "hw3-backend",
  "version": "2.0.0",
  "description": "Backend for diary reflection app with AI categorization - Modern TypeScript + Prisma",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec npx ts-node src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "npx ts-node src/scripts/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run seed"
  },
  "keywords": [
    "diary",
    "ai",
    "reflection",
    "typescript",
    "prisma"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
