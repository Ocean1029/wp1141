// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Placeholder model to enable Prisma Client generation
// This can be removed or replaced when actual models are added
model Placeholder {
  id String @id @default(cuid())
}
